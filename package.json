{
  "name": "cars-g",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-icons && npm run generate-pwa-assets && vite build",
    "generate-pwa-assets": "node scripts/generate-pwa-assets.js",
    "generate-icons": "node scripts/icons.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "prepare": "husky install",
    "analyze": "vite-bundle-analyzer",
    "deploy:check": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 check",
    "deploy:install": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 install",
    "deploy:build": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 build",
    "deploy:env": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 env",
    "deploy:test": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 test",
    "deploy:checklist": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 checklist",
    "deploy:full": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 full",
    "server:install": "cd server && npm install",
    "server:start": "cd server && npm start",
    "server:dev": "cd server && npm run dev",
    "test:deployment": "powershell -ExecutionPolicy Bypass -File scripts/test-deployment.ps1",
    "test:deployment:frontend": "powershell -ExecutionPolicy Bypass -File scripts/test-deployment.ps1 -SkipBackend -SkipDatabase -SkipCloudinary -SkipPerformance",
    "test:deployment:backend": "powershell -ExecutionPolicy Bypass -File scripts/test-deployment.ps1 -SkipFrontend -SkipDatabase -SkipCloudinary -SkipPerformance",
    "test:deployment:database": "powershell -ExecutionPolicy Bypass -File scripts/test-deployment.ps1 -SkipFrontend -SkipBackend -SkipCloudinary -SkipPerformance",
    "test:deployment:cloudinary": "powershell -ExecutionPolicy Bypass -File scripts/test-deployment.ps1 -SkipFrontend -SkipBackend -SkipDatabase -SkipPerformance",
    "test:deployment:performance": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1 -SkipFrontend -SkipBackend -SkipDatabase -SkipCloudinary",
    "test:deployment:verbose": "powershell -ExecutionPolicy Bypass -File scripts/test-deployment.ps1 -Verbose",
    "test:e2e:deployment": "cypress run --spec 'cypress/e2e/deployment.cy.js'",
    "test:e2e:deployment:open": "cypress open --config specPattern='cypress/e2e/deployment.cy.js'",
    "test:health": "curl -f https://cars-g.vercel.app/ && curl -f https://cars-g-api.onrender.com/health",
    "test:quick": "npm run test:deployment:frontend && npm run test:deployment:backend",
    "deploy:backend": "powershell -ExecutionPolicy Bypass -File scripts/deploy-backend.ps1",
    "chat:setup": "npm run server:install && echo 'âœ… Chat backend dependencies installed. Run npm run chat:start to start the server.'",
    "chat:start": "cd server && npm run dev",
    "chat:build": "cd server && npm run build",
    "chat:test": "npm test"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.6",
    "@popperjs/core": "^2.11.8",
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/react-query": "^5.28.4",
    "@tanstack/react-query-devtools": "^5.28.4",
    "@vercel/analytics": "^1.2.2",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "chart.js": "^4.5.0",
    "date-fns": "^3.3.1",
    "dotenv": "^17.2.1",
    "firebase": "^12.2.1",
    "framer-motion": "^11.0.8",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.358.0",
    "react": "^18.3.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.0",
    "react-error-boundary": "^4.0.12",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.22.3",
    "socket.io-client": "^4.8.1",
    "workbox-window": "^7.0.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@eslint/js": "^8.57.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/google.maps": "^3.55.4",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.18",
    "cypress": "^13.6.6",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.4.2",
    "typescript-eslint": "^0.0.1-alpha.0",
    "vite": "^7.1.4",
    "vite-bundle-analyzer": "^0.9.0",
    "vite-plugin-pwa": "^0.19.2",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  }
}
